<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.reportsController">
    <children>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Label text="Reports">
                    <VBox.margin>
                        <Insets bottom="40.0" left="10.0" right="10.0" top="10.0" />
                    </VBox.margin>
                    <font>
                        <Font size="36.0" />
                    </font>
                </Label>
                <Button mnemonicParsing="false" onAction="#appointmentEvent" prefWidth="500.0" text="Appointments By Type">
                    <VBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                    <font>
                        <Font size="24.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#scheduleEvent" prefWidth="500.0" text="Consultant Schedules">
                    <VBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                    <font>
                        <Font size="24.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#countEvent" prefWidth="500.0" text="Total Appointment Count">
                    <VBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                    <font>
                        <Font size="24.0" />
                    </font>
                </Button>
            <Button mnemonicParsing="false" onAction="#backEvent" prefWidth="500.0" text="Back">
               <font>
                  <Font size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="40.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
